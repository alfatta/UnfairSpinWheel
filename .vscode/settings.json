{
  "vite.devCommand": "npm run dev",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    },
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "file",
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "*/assets/*",
    "package.json"
  ],
  "cSpell.words": [
    "Animista",
    "confirmationservice",
    "confirmpopup",
    "cssanimista",
    "decrementbuttonicon",
    "dialogservice",
    "dismissable",
    "dynamicdialog",
    "incrementbuttonicon",
    "inputgroup",
    "inputnumber",
    "inputtext",
    "inputwrapper",
    "Mochiy",
    "noround",
    "primeflex",
    "primeicons",
    "primevue",
    "scrollpanel",
    "tabpanel",
    "tabview",
    "toastservice",
    "useconfirm",
    "usedialog"
  ]
}
